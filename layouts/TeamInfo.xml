<root>
    <Section>
        <Section id="teamSection">
            <Title id="team" text="{self.teamName}"/>
            <Block id="win_background"/>
            <Label id="win" text="{self.win}"/>
        </Section>
        
        <Section id="table">
            
            <Section id="heros">
                <Image src="{self.hero_url[0][0]}" id="hero_img"/>
